<h2>Show user</h2>

<%= link "Back", to: user_path(@conn, :index), class: "btn btn-info btn-xs" %>

<ul>

  <li>
    <strong>Username:</strong>
    <%= @user.username %>
  </li>

  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>

  <li>
    <strong>Password digest:</strong>
    <%= @user.password_digest %>
  </li>

</ul>

<%= link "Edit", to: user_path(@conn, :edit, @user), class: "btn btn-default btn-xs" %>
